{
    "version": 2,
    "build": {
      "env": {
        "PYTHON_VERSION": "3.12"
      },
      "buildCommand": "sh build_files.sh"
    },
    "rewrites": [
      {
        "source": "/static/(.*)",
        "destination": "/static/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/youtube_project/wsgi.py"
      }
    ]
  }